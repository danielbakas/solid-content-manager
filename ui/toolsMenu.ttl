@prefix : <#>.
@prefix ui: <http://www.w3.org/ns/ui#>.
@prefix ind: <../apps/MyAddressBook/index.ttl#>.
@prefix in: <../apps/MyLongChat/index.ttl#>.
@prefix inde: <../apps/MyNotepad/index.ttl#>.
@prefix i: <../apps/MyTracker/index.ttl#>.

:AddressBook a ui:SolidOSLink; ui:dataSource ind:this; ui:label "Address Book".

:Chat a ui:SolidOSLink; ui:dataSource in:this; ui:label "Chat".

:EditPreferences
    a ui:SolidOSLink;
    ui:dataSource </profile/settings/prefs.ttl>;
    ui:label "Edit Preferences";
    ui:pane "basicPreferences".
:EditProfile
    a ui:SolidOSLink;
    ui:dataSource "http://localhost:3101/profile/card";
    ui:label "Edit Profile";
    ui:pane "editProfile".
:Notepad a ui:SolidOSLink; ui:dataSource inde:this; ui:label "Notepad".

:ToolsMenu
    a ui:ButtonListMenu;
    ui:dataSource ( :AddressBook :Tracker :Chat :EditProfile :EditPreferences ).
:Tracker a ui:SolidOSLink; ui:dataSource i:this; ui:label "Task Tracker".

