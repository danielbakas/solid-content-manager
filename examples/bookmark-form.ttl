@prefix : <#> .
@prefix ui: <http://www.w3.org/ns/ui#> .
@prefix bk: <http://www.w3.org/2002/01/bookmark#>. 

:this a ui:Form ;
  ui:formSubject <some.ttl#this> ;
  ui:parts(
    :labelField :urlField  
  ) .
  
:labelField a ui:SingleLineTextField ;
  ui:label "Label" ;
  ui:property ui:label .
    
:urlField a ui:SingleLineTextField ;
  ui:label "URL" ;
  ui:property bk:recalls .