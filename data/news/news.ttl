@prefix ui: <http://www.w3.org/ns/ui#> .
@prefix schema: <http://www.schema.org/> .

:News
  a bookm:Topic, ui:FeedList ; 
  rdfs:label "News".

:TechNews
  a bookm:Topic; 
  rdfs:label "Tech News";
  bookm:subTopicOf :News.

:ScienceNews
  a bookm:Topic; 
  rdfs:label "Science News";
  bookm:subTopicOf :News.

:GeneralNews
  a bookm:Topic; 
  rdfs:label "General News";
  bookm:subTopicOf :News.

